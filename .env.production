NODE_ENV=production
PORT=3000

# Redis (set by platform or use external Redis)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Cache
CACHE_TTL=60

# CORS (set to your domain in production)
CORS_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
CORS_CREDENTIALS=false

# Rate limiting
DEXSCREENER_RATE_LIMIT=300
RATE_LIMIT_WINDOW=60000

# WebSocket
WS_UPDATE_INTERVAL=10000

# Upstream
DEXSCREENER_QUERIES=SOL,BONK,WIF,POPCAT,DOGE,PEPE,MOON,INU,SHIB,USDC,USDT,SRM,ORCA,RAY,SOLO,JUP,PYTH,JITO,MEME,BOME
