# Example environment variables for meme-coin-aggregator

# Server
PORT=3000
NODE_ENV=development

# CORS (comma-separated origins, or * for all)
CORS_ORIGINS=*
CORS_CREDENTIALS=false

# Redis (either REDIS_URL or host/port/password)
# REDIS_URL=redis://default:password@host:port
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# API keys (comma separated). Optional; if empty API is public.
API_KEYS=

# Cache
CACHE_TTL=30

# Upstream limits
DEXSCREENER_RATE_LIMIT=300
RATE_LIMIT_WINDOW=60000

# Upstream discovery (more tokens = better aggregation)
DEXSCREENER_QUERIES=SOL,BONK,WIF,POPCAT,DOGE,PEPE,MOON,INU,SHIB,USDC,USDT,SRM,ORCA,RAY,SOLO,JUP,PYTH,JITO,MEME,BOME
JUPITER_QUERIES=SOL,USDC,BONK,WIF,DOGE,PEPE,MOON,INU,SHIB,USDT,SRM,ORCA,RAY,JUP,PYTH

# WebSocket
WS_UPDATE_INTERVAL=5000

# Scheduler
CRON_SCHEDULE=*/30 * * * * *

# Feature toggles
DEV_EXPAND_UPSTREAM=false
GECKO_ENABLED=false

# CoinGecko (optional)
COINGECKO_API_KEY=
